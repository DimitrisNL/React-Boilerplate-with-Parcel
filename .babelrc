{
  "env": {
    "production": {
      "presets": ["env", "react", "stage-0"],
      "plugins": [
        ["emotion", { "hoist": true }],
        ["module-resolver", { "root": ["./src"] }]
      ]
    },
    "development": {
      "presets": ["env", "react", "stage-0"],
      "plugins": [
        ["emotion", { "sourceMap": true, "autoLabel": true }],
        ["module-resolver", { "root": ["./src"] }]
      ]
    }
  }
}
